native print<A>(x: A): ()

property<A> A.show(): ()

extend<A extends show> (c: [A]) with
  show(): () => print(c)

extend (c: int) with
  show(): () => print(c)

test<A extends show>(x: [A]): () => x.show()

[[1, 2]].show()

infixr 5 $

operator $ <A, B>(f: (A) -> B, x: A): B => f(x)

print([1, 2].test())
