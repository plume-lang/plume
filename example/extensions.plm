native int_to_str(x: int): str
native ffi_print(x: str): unit
native ffi_println(x: str): unit

declare fn display<A>(x: A): str

extend fn display(x: int): str => int_to_str(x)
extend fn display(x: str): str => x
extend fn display(x: bool): str => if x then "true" else "false"

fn print<A>(x: A) => ffi_print(x.display())
fn println<A>(x: A) => ffi_println(x.display())

fn main() {
  println("tset")
  println(42)
  println(true)
  println(false)

  return 0
}