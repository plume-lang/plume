native "std:native" print(x: str): int
native "std:native" println<A>(x: A): int
native "std:native" get_args(): [str]
native "std:native" does_file_exist(path: str): bool
native "std:native" execute_command(path: str): str

@print(x) =>
  print(x.show())

@println(x) =>
  @print(x)
  print("\n")

@print_with(fmt, x) =>
  print(x.show(fmt))

@println_with(fmt, x) =>
  @print_with(fmt, x)
  print("\n")
